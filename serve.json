{
      "headers": [
            {
                  "source": "/_next/static/**/*",
                  "headers": [
                        {
                              "key": "Cache-Control",
                              "value": "public, max-age=31536000, immutable"
                        }
                  ]
            },
            {
                  "source": "/**/*.{jpg,jpeg,png,gif,webp,svg,woff2}",
                  "headers": [
                        {
                              "key": "Cache-Control",
                              "value": "public, max-age=31536000, immutable"
                        }
                  ]
            }
      ],
      "cleanUrls": true,
      "trailingSlash": true
}